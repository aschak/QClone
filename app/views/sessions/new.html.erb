<h1>Sign In</h1>



<form class="user-login-form" action="<%= session_url %>" method="post">

  <%= auth_token %>

  <label>
    Username
  <input
    type="text"
    name="user[username]"
    value="">
  </label>

  <label>
    Password
  <input
    type="password"
    name="user[password]"
    value="">
  </label>

  <input
    class="btn btn-primary"
    type="submit"
    value="Sign In">
</form>

<form class="user-login-form" action="<%= session_url %>" method="post">
  <%= auth_token %>

  <input
    type="hidden"
    name="user[username]"
    value="Guest_Seeker">

  <input
    type="hidden"
    name="user[password]"
    value="AlwaysSeeking">

  <input
    class="btn btn-primary"
    type="submit"
    value="Guest Login">
</form>
